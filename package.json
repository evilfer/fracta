    "@types/eslint__js": "8.42.3",
    "@types/jest": "29.5.14",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "esbuild": "0.24.0",
    "eslint": "9.14.0",
    "husky": "^9.1.6",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-jest": "29.2.5",
